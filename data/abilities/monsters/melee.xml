<root type="ability" class="melee">

  <effect type="list">
    <effect type="damage">
      <damage-type value="physical"/>
      <amount calc="1.0 * strength" />
    </effect>
    <effect type="knockback">
      <amount value="1.0"/>
    </effect>
  </effect>    
  
  <cast-time-effect type="modify">
    <aspect value="speed" />
    <amount value="0.0" />
  </cast-time-effect>
  
  <delay-effect type="modify">
    <aspect value="speed" />
    <amount value="0.0" />
  </delay-effect>
  
  <casttime value="0.2" />  
  <range value="0.7" />
  <width value="90.0" />
  <delay value="0.2" />
  <cooldown value="0.75" />

  </root>
