<root type="ability" class="melee">

  <effect type="list">
    <effect type="damage">
      <damage-type value="physical"/>
      <amount calc="1.0 * strength" />
    </effect>
    <effect type="knockback">
      <amount value="1"/>
    </effect>
    <effect type="graphical-effect">
      <name value="colorize-green" /> 
      <time calc="6.0" /> 
    </effect>
    <effect type="non-cumulative">
      <effect type="timed">
      <time value="6.0" />
        <effect type="damage">
          <damage-type value="poison"/>
          <amount calc="1.0 * poisonous" />
        </effect>
      </effect>
    </effect>
  </effect> 
  
  <cast-time-effect type="modify">
    <aspect value="speed" />
    <amount value="0.0" />
  </cast-time-effect>
  
  <delay-effect type="modify">
    <aspect value="speed" />
    <amount value="0.0" />
  </delay-effect>
  
  <range value="0.7" />
  <width value="90.0" />

  <cast-time value="0.2" />
  <delay value="0.2" />
  <cooldown value="0.75" />
  
</root>
